<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="event">

	<select id="event" resultType="com.koitt.jardin.dto.event.EventDTO">
	
	select * from event
	
	</select>
	
	<select id="eventView" resultType="com.koitt.jardin.dto.event.EventDTO">
	
	select * from event where event_no=#{event_no}
	</select>
	
	<select id="finEvent" resultType="com.koitt.jardin.dto.event.EventDTO">
	
	</select>
	
	<select id="finEventView" resultType="com.koitt.jardin.dto.event.EventDTO">
	
	</select>
	
	<select id="prizeWinner" resultType="com.koitt.jardin.dto.event.WinnerDTO">
	
	</select>
	
	<select id="prizeWinnerView" resultType="com.koitt.jardin.dto.event.WinnerDTO">
	
	</select>
	
</mapper>