<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="community">
<!-- 체험단 글 리스트 -->
	<select id="expr" resultType="com.koitt.jardin.dto.community.PreUserDTO">
		select * from preuser
	</select>
<!-- 	체험단 글 보기 -->
	<select id="exprReview" resultType="com.koitt.jardin.dto.community.PreUserDTO">
		select * from preuser where preUserNo=#{preUserNo}
	</select>
<!-- 	체험단 신청 글 쓰기 -->
	<insert id="preUserReview" >
	insert into preuserReview 
	</insert>
	
	<insert id="preUserApply" >
	insert into preuserApply 
	</insert>
</mapper>